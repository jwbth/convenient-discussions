{
  "name": "convenient-discussions",
  "version": "0.0.0",
  "description": "A JavaScript shell for wiki talk pages.",
  "scripts": {
    "build": "node buildConfigs && cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n && cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "build16": "node buildConfigs && node buildI18n && webpack",
    "start": "node buildConfigs && cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n && cross-env NODE_OPTIONS=--max-old-space-size=2048 NODE_OPTIONS=--openssl-legacy-provider webpack serve --env dev",
    "serve": "cross-env NODE_OPTIONS=--max-old-space-size=2048 NODE_OPTIONS=--openssl-legacy-provider webpack serve --env dev",
    "configs": "node buildConfigs",
    "deploy": "node --unhandled-rejections=strict deploy",
    "docs": "rimraf dist/docs && jsdoc --configure jsdoc/config.js --readme jsdoc/home.md",
    "i18n": "cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n",
    "mirror": "git push --mirror https://gerrit.wikimedia.org/r/mediawiki/gadgets/ConvenientDiscussions",
    "webpack": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "single": "node buildConfigs && cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n && cross-env NODE_OPTIONS=--max-old-space-size=2048 NODE_OPTIONS=--openssl-legacy-provider webpack serve --env single",
    "test": "jest"
  },
  "browser": "src/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwbth/convenient-discussions.git"
  },
  "author": "Jack who built the house",
  "license": "MIT",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/convenient-discussions/"
  },
  "homepage": "https://commons.wikimedia.org/wiki/User:Jack_who_built_the_house/Convenient_Discussions",
  "dependencies": {
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.4",
    "dayjs": "^1.10.5",
    "htmlparser2": "^6.1.0",
    "locutus": "^2.0.15",
    "lz-string": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "@types/jquery": "^3.5.29",
    "@types/oojs": "^7.0.6",
    "@types/oojs-ui": "^0.49.0",
    "@webpack-cli/serve": "^1.3.1",
    "ajv": "^6.12.6",
    "babel-loader": "^8.2.2",
    "banner-webpack-plugin": "^0.2.3",
    "chalk": "^2.4.2",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "docdash": "^1.2.0",
    "dompurify": "^2.5.4",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.3",
    "http-proxy-to-socks": "^1.1.2",
    "jest": "^27.0.3",
    "jsdoc": "^3.6.6",
    "jsdom": "^16.5.2",
    "json5": "^2.2.2",
    "less": "^3.13.1",
    "less-loader": "^5.0.0",
    "node-fetch": "^3.2.10",
    "nodemw": "^0.13.0",
    "prompts": "^2.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^2.3.8",
    "types-mediawiki": "^1.6.0",
    "webpack": "^4.46.0",
    "webpack-build-notifier": "^2.1.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^5.2.1",
    "windows-iana": "^5.0.2",
    "worker-loader": "^3.0.8",
    "yargs": "^15.4.1"
  },
  "sideEffects": [
    "./src/worker.js"
  ],
  "browserslist": [
    "> 1%",
    "not IE 11"
  ]
}
