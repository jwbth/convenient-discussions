  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "https://en.wikipedia.org/wiki/Talk:Main_Page",
      "webRoot": "${workspaceFolder}/dist",
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceRoot}/*"
      },
      "outFiles": [
        "${workspaceFolder}/src/**/*.js",
        "!**/node_modules/**"
      ]
    }
  ]
}
